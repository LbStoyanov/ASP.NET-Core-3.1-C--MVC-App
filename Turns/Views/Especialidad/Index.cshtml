@model IEnumerable<Turns.Models.Speciality>

<!DOCTYPE html>

<html>

<head>
    <title>Especialidades</title>
</head>

<body>
    <table>
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Description)
                </th>
            </tr>
        </thead>
        <tbody>
            @*Aqui colocaremos, mas adelante, el codigo para mostrar los registros de la base de datos*@
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Description)
                    </td>
                </tr>
            }

        </tbody>
    </table>
</body>

</html>